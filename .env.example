# Home Assistant
HA_URL=http://homeassistant.local:8123
HA_TOKEN=your_long_lived_access_token_here

# Reolink NVR
REOLINK_NVR_IP=192.168.x.x
REOLINK_USER=admin
REOLINK_PASS=your_password_here

# MQTT (HA Mosquitto broker)
MQTT_HOST=homeassistant.local
MQTT_USER=homeassistant
MQTT_PASS=your_mqtt_password_here

# Frigate environment variables (used in config.yml)
FRIGATE_NVR_IP=192.168.x.x
FRIGATE_RTSP_PASSWORD=your_reolink_password_here
FRIGATE_MQTT_PASSWORD=your_mqtt_password_here
